<div class="container">
  <img src="/LogoVitalHome.png" alt="Logo VitalHome" />
  <h2>Listado de Profesionales</h2>

  <div class="filters">
    <select id="region">
      <option value="">Todas las regiones</option>
    </select>
    <select id="comuna">
      <option value="">Todas las comunas</option>
    </select>
    <select id="profesion">
      <option value="">Todas las profesiones</option>
    </select>
  </div>

  <table id="tablaProfesionales">
    <thead>
      <tr>
        <th>RUT</th>
        <th>Nombre</th>
        <th>Apellidos</th>
        <th>Profesión</th>
        <th>Registro SIS</th>
        <th>Región</th>
        <th>Comuna</th>
        <th>Fecha Ingreso</th>
        <th>Activo</th>
      </tr>
    </thead>
    <tbody></tbody>
  </table>

  <div class="no-data" id="noData" style="display:none;">
    No hay profesionales que coincidan con los filtros.
  </div>
</div>